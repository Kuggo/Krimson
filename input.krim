nat var = 1
nat ver = 2
nat vir = var + ver

nat collatz(nat n, nat i) {
    if n == 1
        return i

    if n & 1 == 0
        return collatz(n >> 1 i+1)
    else
        return collatz(n*3 + 1 i+1)
}

